#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../react-scripts/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules/webpack/bin/webpack.js" "$@"
  ret=$?
else 
  node  "$basedir/../react-scripts/node_modules/@pmmmwh/react-refresh-webpack-plugin/node_modules/webpack/bin/webpack.js" "$@"
  ret=$?
fi
exit $ret
